                                                                <Button mnemonicParsing="false" onAction="#importFile"
                                                                        prefHeight="25.0" prefWidth="72.0"
                                                                        text="Import"/>
                                                                <Button mnemonicParsing="false" onAction="#exportFile"
                                                                        prefHeight="25.0" prefWidth="72.0"
                                                                        text="Export"/>
                                                            </children>
                                                        </HBox>
                                                    </children>
                                                </AnchorPane>
                                                <AnchorPane prefHeight="250.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
