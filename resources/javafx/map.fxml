                        </AnchorPane>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
            <Separator maxWidth="-Infinity" minWidth="-Infinity" orientation="VERTICAL" prefWidth="0.0" HBox.hgrow="NEVER" />
