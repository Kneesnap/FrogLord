<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.TextFlow?>
<AnchorPane fx:id="editorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="500.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ChoiceBox fx:id="scriptSelector" layoutX="14.0" layoutY="14.0" prefWidth="150.0" AnchorPane.leftAnchor="14.0"
                   AnchorPane.topAnchor="14.0"/>
        <TextFlow fx:id="codeArea" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="25.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
        <Button fx:id="doneButton" mnemonicParsing="false" text="Done" AnchorPane.bottomAnchor="0.0"
                AnchorPane.rightAnchor="0.0"/>
        <VBox fx:id="commandEditors" layoutX="14.0" layoutY="39.0" prefHeight="230.0" prefWidth="100.0"
              AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="40.0"/>
        <Label fx:id="warningLabel" layoutX="14.0" layoutY="479.0"
               text="WARNING: This script is too large. It will most likely fail." textFill="RED" underline="true"
               visible="false"/>
        <Button fx:id="printUsagesButton" layoutX="404.0" layoutY="14.0" mnemonicParsing="false" text="Print Usages"
                AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0"/>
    </children>
</AnchorPane>
