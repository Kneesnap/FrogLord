        <ChoiceBox fx:id="scriptSelector" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="200.0"
                   AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0"/>
