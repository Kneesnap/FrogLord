<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="anchorPaneUIRoot" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="250.0"
            xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <Accordion fx:id="accordionLeft" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <panes>
          <TitledPane fx:id="paneAnim" animated="false" text="Preview Settings">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                      <Label layoutX="48.0" layoutY="49.0" text="Model: " AnchorPane.leftAnchor="0.0"
                             AnchorPane.topAnchor="0.0"/>
                      <ComboBox fx:id="animationSelector" layoutX="83.0" layoutY="6.0" prefWidth="150.0"
                                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="88.4" AnchorPane.topAnchor="25.0"/>
                      <Button fx:id="playButton" layoutX="196.0" layoutY="35.0" mnemonicParsing="false" text="Play"
                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0"/>
                      <CheckBox fx:id="repeatCheckbox" layoutX="10.0" layoutY="85.0" mnemonicParsing="false"
                                selected="true" text="Repeat" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0"/>
                      <Label layoutX="100" layoutY="122" text="FPS: " AnchorPane.rightAnchor="30.0"
                             AnchorPane.topAnchor="73.0"/>
                      <TextField fx:id="fpsField" alignment="CENTER_RIGHT" layoutX="174.0" layoutY="90.0"
                                 prefHeight="25.0" prefWidth="30.0" text="??" AnchorPane.rightAnchor="0.0"
                                 AnchorPane.topAnchor="70.0"/>
                      <Label fx:id="modelName" layoutX="48.0" layoutY="10.0" text="&lt;Model Name&gt;"
                             AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="0.0"/>
                      <Button fx:id="btnLast" layoutX="10.0" layoutY="64.0" mnemonicParsing="false"
                              onAction="#lastFrame" text="&lt;-" AnchorPane.leftAnchor="0.0"
                              AnchorPane.topAnchor="55.0"/>
                      <Button fx:id="btnNext" layoutX="120.0" layoutY="67.0" mnemonicParsing="false"
                              onAction="#nextFrame" prefHeight="25.0" prefWidth="30.0" text="-&gt;"
                              AnchorPane.topAnchor="55.0"/>
                      <Label fx:id="frameLabel" alignment="CENTER" layoutX="40.0" layoutY="71.0" prefHeight="17.0"
                             prefWidth="80.0" text="Frame ?" AnchorPane.topAnchor="60.0"/>
                      <CheckBox fx:id="brightModeCheckbox" layoutX="4.0" layoutY="96.0" mnemonicParsing="false"
                                text="Bright Mode" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="85.0"/>
                      <CheckBox fx:id="viewBoundingBoxesCheckbox" layoutX="13.0" layoutY="153.0" mnemonicParsing="false"
                                text="Show Bounding Boxes" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="100.0"/>
                  </children>
               </AnchorPane>
           </content>
         </TitledPane>
          <TitledPane fx:id="generalPane" animated="false" text="Texture Animation">
              <content>
                  <ScrollPane fitToWidth="true">
                      <content>
                          <AnchorPane>
                              <children>
                                  <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                      <children>
                                          <CheckBox fx:id="textureEditCheckbox" mnemonicParsing="false"
                                                    text="Show Texture Animation"/>
                                          <Button fx:id="addTextureAnimationButton" mnemonicParsing="false"
                                                  text="Add Texture Animation"/>
                                          <Separator prefWidth="200.0"/>
                                          <GridPane fx:id="textureEditPane" prefWidth="250.0">
                                              <columnConstraints>
                                                  <ColumnConstraints hgrow="ALWAYS" maxWidth="110.0" minWidth="10.0"
                                                                     prefWidth="100.0"/>
                                                  <ColumnConstraints hgrow="ALWAYS" maxWidth="140.0" minWidth="10.0"
                                                                     prefWidth="150.0"/>
                                              </columnConstraints>
                                          </GridPane>
                                      </children>
                                  </VBox>
                              </children>
                          </AnchorPane>
                      </content>
                      <padding>
                          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                      </padding>
                  </ScrollPane>
              </content>
          </TitledPane>
          <TitledPane fx:id="collprimPane" animated="false" text="Collprim Editor">
              <content>
                  <ScrollPane fitToWidth="true">
                      <content>
                          <AnchorPane>
                              <children>
                                  <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                      <children>
                                          <CheckBox fx:id="viewCollprimCheckbox" mnemonicParsing="false"
                                                    text="Show Collprim"/>
                                          <Button fx:id="addCollprimButton" mnemonicParsing="false"
                                                  text="Add Collprim"/>
                                          <Separator prefWidth="200.0"/>
                                          <GridPane fx:id="collprimEditPane" prefWidth="250.0">
                                              <columnConstraints>
                                                  <ColumnConstraints hgrow="ALWAYS" maxWidth="110.0" minWidth="10.0"
                                                                     prefWidth="100.0"/>
                                                  <ColumnConstraints hgrow="ALWAYS" maxWidth="140.0" minWidth="10.0"
                                                                     prefWidth="150.0"/>
                                              </columnConstraints>
                                          </GridPane>
                                      </children>
                                  </VBox>
                              </children>
                          </AnchorPane>
                      </content>
                      <padding>
                          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                      </padding>
                  </ScrollPane>
              </content>
          </TitledPane>
          <TitledPane fx:id="hilitePane" animated="false" text="Hilite Editor">
              <content>
                  <ScrollPane fitToWidth="true">
                      <content>
                          <AnchorPane>
                              <children>
                                  <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                      <children>
                                          <CheckBox fx:id="viewHilitesCheckbox" mnemonicParsing="false"
                                                    text="Show Hilites"/>
                                          <Button fx:id="addHiliteButton" mnemonicParsing="false"
                                                  text="Add New Hilite"/>
                                          <Separator prefWidth="200.0"/>
                                          <GridPane fx:id="hiliteEditPane" prefWidth="250.0">
                                              <columnConstraints>
                                                  <ColumnConstraints hgrow="ALWAYS" maxWidth="110.0" minWidth="10.0"
                                                                     prefWidth="100.0"/>
                                                  <ColumnConstraints hgrow="ALWAYS" maxWidth="140.0" minWidth="10.0"
                                                                     prefWidth="150.0"/>
                                              </columnConstraints>
                                          </GridPane>
                                      </children>
                                  </VBox>
                              </children>
                          </AnchorPane>
                      </content>
                      <padding>
                          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                      </padding>
                  </ScrollPane>
              </content>
          </TitledPane>
      </panes></Accordion>
</AnchorPane>
