      <panes>
         <TitledPane fx:id="paneAnim" animated="false" text="Animation Data">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                      <Label layoutX="48.0" layoutY="49.0" text="Model: " AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                      <ComboBox fx:id="animationSelector" layoutX="83.0" layoutY="6.0" prefWidth="150.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="88.4" AnchorPane.topAnchor="25.0" />
                      <Button fx:id="playButton" layoutX="196.0" layoutY="35.0" mnemonicParsing="false" text="Play" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0" />
                      <CheckBox fx:id="repeatCheckbox" layoutX="10.0" layoutY="85.0" mnemonicParsing="false" selected="true" text="Repeat" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0" />
